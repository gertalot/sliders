{
  "$schema": "https://typedoc-plugin-markdown.org/schema.json",
  "entryPoints": [
    "../sliders/src/hooks/",
    "../sliders/src/utils/"
  ],
  "plugin": ["typedoc-plugin-markdown"],
  "out": "../docs/docs/api",
  "commentStyle": "all",
  "includeVersion": true,
  "navigation": {
      "includeCategories": true,
      "includeGroups": false,
      "includeFolders": true
  },
  "categoryOrder": ["hooks", "utils"],
  "defaultCategory": "other",
  "categorizeByGroup": false,
  "name": "@gertalot/sliders",
  "headings": false,
  "externalPattern": [
    "**/node_modules/**"
  ]

}